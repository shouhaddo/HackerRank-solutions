   "source": [
    "from itertools import combinations\n",
    "lst = list(map(str,input().rstrip().split()))\n",
    "\n",
    "\n",
    "for i in range(1,int(lst[1])+1):\n",
    "    for word in combinations(sorted(lst[0]),i):\n",
    "        \n",
    "        print(\"\".join(word))"
   ]
